package com.javalec.base;

public class Review1 {
	public static void main(String[] args) {
	
	//Review
	System.out.println("Hello"+"World");
	System.out.println("World");
	
	int value1=100;
	int value2=300;
	
	System.out.println("value1 = " + value1);
	System.out.println("value2 = " + value2);
	
	System.out.println("덧셈 : " + value1 + " + " + value2 + " = " + (value1+value2));
	System.out.println("뺄셈 : " + value2 + " - " + value1 + " = " + (value2-value1));
	System.out.println("곱셈 : " + value1 + " * " + value2 + " = " + (value1*value2));
	System.out.println("나눗셈(몫) : " + value2 + " / " + value1 + " = " + (value2/value1));
	
	System.out.println(" *  * ");
	System.out.println("*** ***");
	System.out.println("*******");
	System.out.println("  *** ");
	System.out.println("   *  ");
	}
}
